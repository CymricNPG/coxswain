<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:coxswain="http://schemas.android.com/apk/res-auto"
    >
    <item coxswain:value_low="true">
        <inset>
            <shape>
                <solid
                    android:color="#80AA0000"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
    <item coxswain:value_high="true">
        <inset>
            <shape>
                <solid
                    android:color="#8028FF00"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
    <item coxswain:value_target="true">
        <inset>
            <shape>
                <solid
                    android:color="#403567ed"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
</selector>
