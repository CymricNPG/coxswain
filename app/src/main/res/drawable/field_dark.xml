<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:coxswain="http://schemas.android.com/apk/res-auto"
    >
    <item coxswain:value_low="true">
        <inset>
            <shape>
                <!--80AA0000 on black-->
                <solid
                    android:color="#340000"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
    <item coxswain:value_high="true">
        <inset>
            <shape>
                <!--8028FF00 on black-->
                <solid
                    android:color="#0c4f00"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
    <item coxswain:value_target="true">
        <inset>
            <shape>
                <!--403567ed on black-->
                <solid
                    android:color="#101f49"
                />
                <corners android:radius="5dp" />
            </shape>
        </inset>
    </item>
</selector>
